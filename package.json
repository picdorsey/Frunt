{
  "name": "frunt",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Scaffolds out a Frunt & gulp.js boilerplate for front-end development",
  "author": "Piccirilli Dorsey, inc",
  "repository": {},
  "scripts": {
    "dev": "npm run development",
    "development": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "NODE_ENV=development webpack-dev-server --inline --hot",
    "prod": "npm run production",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "browser": {},
  "devDependencies": {
    "axios": "^0.16.2",
    "import-glob-loader": "^1.1.0",
    "jquery": "^3.1.1",
    "laravel-mix": "^1.0",
    "lodash": "^4.17.4",
    "vue": "^2.4.0"
  },
  "dependencies": {
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.2.0"
  }
}
